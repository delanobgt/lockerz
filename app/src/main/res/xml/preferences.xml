<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="3"
        android:entries="@array/workerCounts"
        android:entryValues="@array/workerCounts"
        android:key="workerCount"
        android:summary="Set Crypt Worker count"
        android:title="Number of Crypt Worker" />
    <ListPreference
        android:defaultValue="CAESAR"
        android:entries="@array/encryptionTypes"
        android:entryValues="@array/encryptionTypes"
        android:key="encryptionType"
        android:summary="Set defaultEncryptionType"
        android:title="Default Encryption Type" />
</android.support.v7.preference.PreferenceScreen>